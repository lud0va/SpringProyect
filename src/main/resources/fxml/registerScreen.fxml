            prefHeight="400.0" prefWidth="600.0">
    <children>

        <TextField fx:id="txtUser" layoutX="200.0" layoutY="121.0" promptText="Username"/>
        <StackPane fx:id="txtPassword" prefHeight="150.0" prefWidth="200.0"/>
        <TextField layoutX="201.0" layoutY="187.0" promptText="Password"/>
        <Button layoutX="139.0" layoutY="256.0" mnemonicParsing="false" text="Login"/>
        <Button layoutX="354.0" layoutY="256.0" mnemonicParsing="false" text="Register"/>
    </children>

</AnchorPane>
